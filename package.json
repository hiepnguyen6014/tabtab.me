{
  "name": "nextjs",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=\"--max-old-space-size=6000\" next",
    "build-dev": "env-cmd -f .env.development next build",
    "build-test": "env-cmd -f .env.test next build",
    "build-prepro": "env-cmd -f .env.prepro next build",
    "build": "env-cmd -f .env.production next build",
    "start": "next start",
    "lint": "eslint --ignore-path .gitignore src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --fix --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --ignore-path .gitignore --write './src/**/*.{js,jsx,ts,tsx,css,md,json}'",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json}": [
      "npm run lint"
    ],
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "npm run format"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@emotion/core": "^11.0.0",
    "@emotion/css": "^11.5.0",
    "@emotion/react": "^11.6.0",
    "@emotion/styled": "^11.6.0",
    "@fontsource/inter": "^4.5.11",
    "@moxy/next-layout": "^2.2.3",
    "@types/lodash": "^4.14.178",
    "antd": "^4.16.13",
    "axios": "^0.24.0",
    "chart.js": "^3.7.0",
    "env-cmd": "^10.1.0",
    "express": "^4.17.1",
    "moment": "^2.29.1",
    "next": "^13.0.2",
    "next-cookies": "^2.0.3",
    "next-i18next": "^9.2.0",
    "nextjs-progressbar": "^0.0.13",
    "qs-stringify": "^1.2.1",
    "react": "^18.2.0",
    "react-chartjs-2": "^4.0.0",
    "react-color": "^2.19.3",
    "react-cookie": "^4.1.1",
    "react-dom": "^18.2.0",
    "react-facebook": "^8.1.4",
    "react-facebook-login": "^4.1.1",
    "react-google-recaptcha-v3": "^1.9.7",
    "react-hook-form": "^7.20.2",
    "react-i18next": "^11.14.2",
    "react-quill": "^2.0.0",
    "react-share": "^4.4.0",
    "sass": "^1.54.3",
    "swiper": "^8.4.4"
  },
  "devDependencies": {
    "@svgr/webpack": "^5.5.0",
    "@trivago/prettier-plugin-sort-imports": "^3.4.0",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@types/reactstrap": "^8.7.2",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-less": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "babel": "^6.23.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.26.0",
    "eslint-config-next": "^13.0.2",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "husky": "^8.0.1",
    "less": "^4.1.2",
    "less-loader": "^4.1.0",
    "lint-staged": "^13.0.3",
    "node-sass": "^6.0.1",
    "prettier": "^2.7.1",
    "react-svg-loader": "^3.0.3",
    "sass-loader": "^12.3.0",
    "typescript": "4.5.2",
    "webpack": "^5.64.2"
  }
}
